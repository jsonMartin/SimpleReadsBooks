---
import ButtonBack from "../components/ButtonBack.svelte";
---

<head>
  <script
    src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"
  ></script>
  <title>Uh oh</title>
</head>

<body>
  <section
    class="flex h-[100vh] flex-col items-center bg-white dark:bg-gray-900"
  >
    <div
      class="mx-auto my-auto flex h-[100%] max-w-screen-xl items-center px-4 py-8 lg:px-6 lg:py-16"
    >
      <div class="mx-auto max-w-screen-sm text-center">
        <lottie-player
          src="https://lottie.host/5824a45e-3640-4d4f-8085-c82b1a40ca91/4RT9fuVtBk.json"
          background="#fff"
          speed="1"
          class="sm:h-[600px] sm:w-[600px]"
          loop
          autoplay
          direction="1"
          mode="normal"></lottie-player>
        <h1 class="mb-4 text-2xl font-extrabold text-red-600 dark:text-red-500">
          Page Not Found
        </h1>
        <p
          class="mb-10 text-3xl font-bold tracking-tight text-gray-900 dark:text-white md:text-4xl"
        >
          Uh oh! That URL does not exist &#128562;
        </p>
        <ButtonBack client:load />
      </div>
    </div>
  </section>
</body>
