{
  "name": "simplereadsbooks",
  "type": "module",
  "version": "1.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "sync": "rimraf src/pages/posts/* && node src/astronot.js",
    "sync:published": "rimraf src/pages/posts/* && node src/astronot.js --published",
    "generate": "rimraf dist/** && ([ -d 'dist' ] || mkdir dist) && ([ -d 'dist/images' ] || mkdir dist/images) && ([ -d 'src/pages/posts' ] || mkdir src/pages/posts) && ([ -d 'src/images' ] || mkdir src/images) && ([ -d 'src/images/posts' ] || mkdir src/images/posts) && rimraf src/pages/posts/* && node src/astronot.js --published && astro build"
  },
  "dependencies": {
    "@astrojs/mdx": "^1.1.3",
    "@astrojs/netlify": "^3.0.3",
    "@astrojs/svelte": "^4.0.3",
    "@astrojs/tailwind": "^5.0.2",
    "@jsonmartin/svelte-cloudinary-astro": "^1.1.2",
    "@tailwindcss/typography": "^0.5.10",
    "astro": "^3.4.3",
    "flowbite": "^2.0.0",
    "flowbite-svelte": "npm:@jsonmartin/flowbite-svelte@^0.44.17",
    "flowbite-svelte-icons": "^0.4.5",
    "nanostores": "^0.9.4",
    "prettier": "^3.0.3",
    "svelte": "^4.2.2",
    "tailwindcss": "^3.3.5",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.13",
    "dotenv": "^16.3.1",
    "image-type": "^5.2.0",
    "notion-to-md": "^3.1.1",
    "prettier-plugin-astro": "^0.12.1",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "reading-time": "^1.5.0",
    "rimraf": "^5.0.5"
  }
}
